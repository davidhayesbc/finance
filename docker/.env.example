# Copy this file to docker/.env and fill in your values
# NEVER commit .env to source control

# Database
DB_PASSWORD=your-strong-postgres-password-here

# JWT (generate: openssl rand -base64 64)
JWT_SECRET_KEY=your-minimum-32-character-random-secret-key-here

# Application URLs
APP_URL=https://privestio.yourdomain.com
WEB_ORIGIN=https://privestio.yourdomain.com
APP_DOMAIN=privestio.yourdomain.com

# OIDC (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
MICROSOFT_CLIENT_ID=
MICROSOFT_CLIENT_SECRET=
